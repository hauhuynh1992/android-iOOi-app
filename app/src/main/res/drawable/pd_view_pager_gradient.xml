<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:tools="http://schemas.android.com/tools"
xmlns:android="http://schemas.android.com/apk/res/android"
tools:ignore="MissingDefaultResource">

<!-- Drop Shadow Stack -->
<item>
    <shape>
        <padding
            android:bottom="@dimen/_1sdp"
            android:left="@dimen/_1sdp"
            android:right="@dimen/_1sdp"
            android:top="@dimen/_1sdp" />

        <solid android:color="#00DCDEE3" />

        <corners android:radius="@dimen/pd_corner_topRightRadius" />
    </shape>
</item>
<item>
    <shape>
        <padding
            android:bottom="@dimen/_1sdp"
            android:left="@dimen/_1sdp"
            android:right="@dimen/_1sdp"
            android:top="@dimen/_1sdp" />

        <solid android:color="#05DCDEE3" />

        <corners android:radius="@dimen/pd_corner_topRightRadius" />
    </shape>
</item>
<item>
    <shape>
        <padding
            android:bottom="@dimen/_1sdp"
            android:left="@dimen/_1sdp"
            android:right="@dimen/_1sdp"
            android:top="@dimen/_1sdp" />

        <solid android:color="#0aDCDEE3" />

        <corners android:radius="@dimen/pd_corner_topRightRadius" />
    </shape>
</item>
<item>
    <shape>
        <padding
            android:bottom="@dimen/_1sdp"
            android:left="@dimen/_1sdp"
            android:right="@dimen/_1sdp"
            android:top="@dimen/_1sdp" />

        <solid android:color="#0fDCDEE3" />

        <corners android:radius="@dimen/pd_corner_topRightRadius" />
    </shape>
</item>
<item>
    <shape>
        <padding
            android:bottom="@dimen/_1sdp"
            android:left="@dimen/_1sdp"
            android:right="@dimen/_1sdp"
            android:top="@dimen/_1sdp" />

        <solid android:color="#14DCDEE3" />

        <corners android:radius="@dimen/pd_corner_topRightRadius" />
    </shape>
</item>

<!-- Background -->
<item
    android:id="@+id/itemShape">
    <shape>
        <gradient
            android:startColor="@color/pd_view_pager_1"
            android:endColor="@color/pd_view_pager_2"
            android:angle="270"
            />
        <corners android:radius="@dimen/pd_corner_topRightRadius" />
    </shape>
</item>

</layer-list>