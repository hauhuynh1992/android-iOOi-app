<resources>

    <!-- Base application theme. -->
    <style name="SocialAppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/social_colorPrimary</item>
        <item name="colorPrimaryDark">@color/social_colorPrimaryDark</item>
        <item name="colorAccent">@color/social_colorAccent</item>
        <item name="android:statusBarColor">@color/social_colorPrimary</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="bottomSheetDialogTheme">@style/AppBottomSheetDialogTheme</item>
    </style>

    <style name="AppBottomSheetDialogTheme" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="android:windowSoftInputMode">adjustResize|stateVisible</item>
        <item name="bottomSheetStyle">@style/AppModalStyle</item>
    </style>

    <style name="AppModalStyle" parent="Widget.Design.BottomSheet.Modal">
        <item name="android:background">@drawable/rounded_dialog</item>
    </style>

    <style name="SocialFontMedium">
        <item name="fontPath">@string/social_font_medium</item>
    </style>

    <style name="SocialFontBold">
        <item name="fontPath">@string/social_font_bold</item>
    </style>

    <style name="SocialFontRegular">
        <item name="fontPath">@string/social_font_regular</item>
    </style>

    <style name="SocialAppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="SocialAppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="SocialMaterialButtonStyle" parent="Widget.MaterialComponents.Button">
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/Social_font_size_medium</item>
        <item name="android:paddingTop">@dimen/_12sdp</item>
        <item name="android:paddingBottom">@dimen/_12sdp</item>
        <item name="android:paddingStart">@dimen/Social_spacing_standard</item>
        <item name="android:paddingEnd">@dimen/Social_spacing_standard</item>
        <item name="android:theme">@style/Theme.MaterialComponents.Light</item>
        <item name="cornerRadius">@dimen/Social_spacing_middle</item>
        <item name="fontPath">@string/social_font_medium</item>
    </style>

    <style name="SocialMaterialButtonStyle.Filled">
        <item name="backgroundTint">@color/social_colorAccent</item>
        <item name="android:textColor">@color/social_white</item>
        <item name="rippleColor">@color/social_white</item>
    </style>

    <style name="SocialMaterialButtonStyle.Stroked">
        <item name="backgroundTint">@color/social_white</item>
        <item name="strokeColor">@color/social_colorPrimary</item>
        <item name="strokeWidth">@dimen/_1sdp</item>
        <item name="android:textColor">@color/social_colorPrimary</item>
    </style>

    <style name="SocialHeading">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/app_name</item>
        <item name="android:textColor">@color/social_textColorPrimary</item>
        <item name="android:textSize">@dimen/Social_font_size_medium</item>
        <item name="fontPath">@string/social_font_medium</item>
    </style>

    <style name="SocialCard">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginStart">@dimen/_12sdp</item>
        <item name="android:layout_marginTop">@dimen/Social_spacing_standard</item>
        <item name="android:layout_marginEnd">@dimen/_12sdp</item>
        <item name="android:layout_marginBottom">@dimen/Social_spacing_standard</item>
        <item name="android:background">@drawable/social_bg_shadow</item>
        <item name="android:padding">@dimen/_12sdp</item>
    </style>

    <style name="SocialRecycleView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:clipToPadding">false</item>
        <item name="android:overScrollMode">never</item>
    </style>

    <style name="SocialIconCard">
        <item name="android:layout_width">@dimen/_45sdp</item>
        <item name="android:layout_height">@dimen/_45sdp</item>
        <item name="android:background">@drawable/social_app_bg_rectangle</item>
        <item name="android:padding">@dimen/_12sdp</item>
        <item name="android:tint">@color/social_white</item>
    </style>

    <style name="SocialTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/social_textColorPrimary</item>
        <item name="android:textSize">@dimen/Social_font_size_medium</item>
        <item name="fontPath">@string/social_font_medium</item>
    </style>

    <style name="SocialSubTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/social_textColorSecondary</item>
        <item name="android:textSize">@dimen/Social_font_size_medium</item>
    </style>

    <style name="SocialOptionLayout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_marginStart">@dimen/Social_spacing_middle</item>
        <item name="android:layout_marginEnd">@dimen/Social_spacing_standard</item>
    </style>

    <style name="SocialStrokeText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:background">@drawable/social_bg_stroke</item>
        <item name="android:layout_marginStart">@dimen/Social_spacing_standard_new</item>
        <item name="android:layout_marginEnd">@dimen/Social_spacing_standard_new</item>
        <item name="android:layout_marginTop">@dimen/Social_spacing_standard_new</item>
        <item name="android:paddingStart">@dimen/Social_spacing_standard_new</item>
        <item name="android:paddingEnd">@dimen/Social_spacing_standard_new</item>
        <item name="android:paddingTop">@dimen/_14sdp</item>
        <item name="android:paddingBottom">@dimen/_14sdp</item>
        <item name="android:textColor">@color/social_textColorPrimary</item>
        <item name="android:textSize">@dimen/Social_font_size_medium</item>
        <item name="android:drawablePadding">@dimen/Social_spacing_standard</item>
    </style>

    <style name="SocialBtnCustom">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/Social_spacing_standard_new</item>
        <item name="android:background">@drawable/social_bg_primary_corner</item>
        <item name="android:backgroundTint">@color/social_colorAccent</item>
        <item name="android:paddingStart">@dimen/Social_spacing_large</item>
        <item name="android:paddingTop">@dimen/_14sdp</item>
        <item name="android:paddingEnd">@dimen/Social_spacing_large</item>
        <item name="android:paddingBottom">@dimen/_14sdp</item>
        <item name="android:textAppearance">@style/SocialFontMedium</item>
        <item name="android:textColor">@color/social_white</item>
        <item name="android:textSize">@dimen/Social_font_size_medium</item>
    </style>


    // Doctor

